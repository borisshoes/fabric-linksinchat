{
  "schemaVersion": 1,
  "id": "linksinchat",
  "version": "${version}",
  "name": "Links In Chat",
  "description": "A mod that lets you put clickable links in chat and whispers",
  "authors": [
    "BorisShoes"
  ],
  "contact": {
    "repo": "https://github.com/borisshoes/fabric-linksinchat"
  },
  "license": "OSL-3.0",
  "icon": "assets/linksinchat/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "net.borisshoes.linksinchat.Linksinchat"
    ]
  },
  "mixins": [
    "linksinchat.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  }
